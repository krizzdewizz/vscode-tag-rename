{
  "name": "tag-rename",
  "description": "Rename HTML/XML tags",
  "version": "0.2.1",
  "publisher": "krizzdewizz",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "rename",
    "html",
    "xml",
    "vscode",
    "extension"
  ],
  "icon": "doc/logo.png",
  "bugs": {
    "url": "https://github.com/krizzdewizz/vscode-tag-rename/issues"
  },
  "homepage": "https://github.com/krizzdewizz/vscode-tag-rename",
  "repository": {
    "type": "git",
    "url": "https://github.com/krizzdewizz/vscode-tag-rename.git"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:xml",
    "onLanguage:php"
