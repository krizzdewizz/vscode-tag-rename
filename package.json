    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "parse5": "*"
  },
  "devDependencies": {
    "tslint": "^3.8.1",
    "typescript": "^1.8.5",
    "vscode": "^0.11.0",
    "jasmine": "*"
  }
}